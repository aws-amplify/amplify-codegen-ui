{
    "componentType": "Button",
    "name": "UpdateUserAttributeButton",
    "properties": {
      "label": {
        "value": "Sign out"
      }
    },
    "events": {
      "click": {
        "action": "Amplify.AuthUpdateUserAttributes",
        "parameters": {
          "attributes": {
            "email": {
              "value": "newemail@domain.com"
            },
            "displayname": {
              "value": "newdisplayname"
            }
          }
        }
      }
    }
  }
  