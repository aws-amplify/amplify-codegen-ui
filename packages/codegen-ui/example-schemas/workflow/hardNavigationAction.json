{
  "componentType": "Button",
  "name": "NavigateButton",
  "properties": {
    "children": {
      "value": "Go to Amazon.com"
    }
  },
  "events": {
    "click": {
      "action": "Amplify.Navigation",
      "parameters": {
        "type": {
          "value": "url"
        },
        "url": {
          "value": "https://www.amazon.com/"
        }
      }
    }
  }
}
