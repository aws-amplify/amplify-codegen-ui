{
    "componentType": "Flex",
    "name": "SiteHeader",
    "bindingProperties": {
      "openAmplifyDocs": {
        "type": "Action",
        "bindingProperties": {
          "type": "Navigation.Open",
          "parameters": {
            "href": {
              "value": "docs.amplify.aws"
            }
          }
        }
      },
      "redirectToFooter": {
        "type": "Action",
        "bindingProperties": {
          "type": "Navigation.Redirect",
          "parameters": {
            "href": {
              "value": "#footer-id"
            },
            "replaceHistory": {
              "value": true
            }
          }
        }
      },
      "navigateToFAQPage": {
        "type": "Action",
        "bindingProperties": {
        "type": "Navigation.Redirect",
          "parameters": {
            "href": {
              "value": "/faq"
            }
          }
        }
      },
      "reloadPage": {
        "type": "Action",
        "bindingProperties": {
          "type": "Navigation.Refresh"
        }
      }
    },
    "properties": {
      "direction": {
        "value": "row"
      },
      "justifyContent": {
        "value": "space-between"
      }
    },
    "children": [
      {
        "componentType": "Heading",
        "name": "Heading1",
        "properties": {
          "level": {
            "value": 1
          },
          "children": {
            "value": "Title"
          }
        }
      },
      {
        "componentType": "Button",
        "name": "Button1",
        "properties": {
          "click": {
            "bindingProperties": {
              "property": "openAmplifyDocs"
            }
          },
          "variation": {
            "value": "primary"
          },
          "children": {
            "value": "Open Amplify Docs"
          }
        }
      },
      {
        "componentType": "Button",
        "name": "Button2",
        "properties": {
          "click": {
            "bindingProperties": {
              "property": "navigateToFAQPage"
            }
          },
          "variation": {
            "value": "primary"
          },
          "children": {
            "value": "Navigate to FAQ Page"
          }
        }
      },
      {
        "componentType": "Button",
        "name": "Button3",
        "properties": {
          "click": {
            "bindingProperties": {
              "property": "reloadPage"
            }
          },
          "variation": {
            "value": "primary"
          },
          "children": {
            "value": "Reload Page"
          }
        }
      },
      {
        "componentType": "Button",
        "name": "Button4",
        "properties": {
          "click": {
            "bindingProperties": {
              "property": "redirectToFooter"
            }
          },
          "variation": {
            "value": "primary"
          },
          "children": {
            "value": "Redirect To Footer (No History Update)"
          }
        }
      }
    ]
  }
