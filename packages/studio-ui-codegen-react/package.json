    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "jest": {
    "verbose": false,
    "preset": "ts-jest",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**/*.ts"
    ],
    "testRegex": "(lib/__tests__/.*.test.ts)$",
